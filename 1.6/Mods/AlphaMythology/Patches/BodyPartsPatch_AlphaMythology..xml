<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Alpha Mythology</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>

				<li Class="PatchOperationAdd">
					<xpath>/Defs/RecipeDef[@Name= "AI_SurgeryInstallProsthetic_Body"]/appliedOnFixedBodyParts</xpath>
					<value>
						<li>MM_Cytoplasm</li>
						
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/RecipeDef[@Name= "AI_SurgeryInstallBionic_Body"]/appliedOnFixedBodyParts</xpath>
					<value>
						<li>MM_Cytoplasm</li>
						
					</value>
				</li>
			
				<li Class="PatchOperationAdd">
					<xpath>/Defs/RecipeDef[@Name= "AI_SurgeryInstallProsthetic_Wing"]/appliedOnFixedBodyParts</xpath>
					<value>
						<li>MM_Wing</li>
						
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/RecipeDef[@Name= "AI_SurgeryInstallBionic_Wing"]/appliedOnFixedBodyParts</xpath>
					<value>
						<li>MM_Wing</li>
					
					</value>
				</li>
			
				<li Class="PatchOperationAdd">
					<xpath>/Defs/RecipeDef[@Name= "AI_SurgeryInstallProsthetic_Stinger"]/appliedOnFixedBodyParts</xpath>
					<value>
						<li>MM_ScorpionTail</li>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/RecipeDef[@Name= "AI_SurgeryInstallBionic_Stinger"]/appliedOnFixedBodyParts</xpath>
					<value>
						<li>MM_ScorpionTail</li>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/RecipeDef[@Name= "AI_SurgeryInstallProsthetic_Horn"]/appliedOnFixedBodyParts</xpath>
					<value>
						<li>MM_Antlers</li>
						<li>MM_Horns</li>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/RecipeDef[@Name= "AI_SurgeryInstallBionic_Horn"]/appliedOnFixedBodyParts</xpath>
					<value>
						<li>MM_Antlers</li>
						<li>MM_Horns</li>
					</value>
				</li>
	
				<li Class="PatchOperationAdd">
					<xpath>/Defs/HediffDef[defName= "AI_AnimalProstheticStinger"]/modExtensions/li[@Class="AlphaImplants.HediffImplantExtension"]/bodyPartGroupsToMultiplyDamage</xpath>
					<value>
						<li>MM_AttackTail</li>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/HediffDef[defName= "AI_AnimalBionicStinger"]/modExtensions/li[@Class="AlphaImplants.HediffImplantExtension"]/bodyPartGroupsToMultiplyDamage</xpath>
					<value>
						<li>MM_AttackTail</li>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>

