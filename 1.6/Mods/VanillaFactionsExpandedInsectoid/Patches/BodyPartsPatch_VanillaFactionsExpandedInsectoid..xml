<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Factions Expanded - Insectoids</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>

	
				<li Class="PatchOperationAdd">
					<xpath>/Defs/RecipeDef[@Name= "AI_SurgeryInstallProsthetic_Blade"]/appliedOnFixedBodyParts</xpath>
					<value>
						<li>VFEI2_LeftClaw</li>
						<li>VFEI2_RightClaw</li>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/RecipeDef[@Name= "AI_SurgeryInstallBionic_Blade"]/appliedOnFixedBodyParts</xpath>
					<value>
						<li>VFEI2_LeftClaw</li>
						<li>VFEI2_RightClaw</li>
					</value>
				</li>
	

				
				<li Class="PatchOperationAdd">
					<xpath>/Defs/HediffDef[defName= "AI_AnimalProstheticBlade"]/modExtensions/li[@Class="AlphaImplants.HediffImplantExtension"]/bodyPartGroupsToMultiplyDamage</xpath>
					<value>
						<li>VFEI2_ClawAttackTool</li>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/HediffDef[defName= "AI_AnimalBionicBlade"]/modExtensions/li[@Class="AlphaImplants.HediffImplantExtension"]/bodyPartGroupsToMultiplyDamage</xpath>
					<value>
						<li>VFEI2_ClawAttackTool</li>
					</value>
				</li>
				
				
			</operations>
		</match>
	</Operation>
</Patch>

